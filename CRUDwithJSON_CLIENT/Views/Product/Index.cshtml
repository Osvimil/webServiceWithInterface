@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table cellpadding="2" cellspacing="2" border="1">
        <tr>
            <th>id</th>
            <th>name</th>
            <th>price</th>
            <th>quantity</th>
        </tr>
        @foreach (var product in ViewBag.listProducts)
        {
            <tr>

                <td>@product.id</td>
                <td>@product.name</td>
                <td>@product.price</td>
                <td>@product.quantity</td>
            </tr>
        }

    </table>
</body>
</html>
